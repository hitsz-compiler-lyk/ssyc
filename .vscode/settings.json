{
    "files.associations": {
        "*.g4": "antlr",
        "m": "shellscript",
        "*.sy": "c",
        "*.s": "arm",
    },

    "files.exclude": {
        "bin": true,
        "lib": true,
        "_trash": true,
        "target": true,
        "**/.antlr": true,
        "test-data/**/*.exec": true,
    },
    
    "workbench.editor.enablePreview": true,
    "explorer.compactFolders": true,
}